<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.schoolappadmin_v2">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".AdminHome"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".AddEvent" />
        <activity
            android:name=".GalleryPagingActivity"
            android:theme="@style/AppTheme" />
        <activity
            android:name=".GalleryDetailsActivity"
            android:theme="@style/AppTheme" />
        <activity
            android:name=".AddGallery"
            android:theme="@style/AppTheme" />
        <activity android:name=".AddNews" />
        <activity android:name=".AddNotice" />
        <activity android:name=".AddPhoto"></activity>
    </application>

</manifest>